import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc =new Scanner (System.in);
          int num = sc.nextInt();
        int n=sc.nextInt();
        for (int i = 1; i <= n; i++) {
            int result = num * i;
            System.out.println(num + " x " + i + " = " + result);
        }
    }
}

